{
  "name": "misty",
  "version": "1.6.0",
  "description": "Logging utils for command line",
  "author": "Alec Larson",
  "license": "MIT",
  "module": "index.mjs",
  "sideEffects": false,
  "repository": "alloc/misty",
  "prettier": "@alloc/prettier-config",
  "files": [
    "src",
    "dist",
    "*.js",
    "*.js.map",
    "!test.js",
    "!dist/spin.mjs",
    "!dist/task.mjs"
  ],
  "scripts": {
    "build": "rimraf chunk-* && tsup",
    "prepare": "yarn build"
  },
  "dependencies": {
    "kleur": "^4.1.4",
    "shared-log": "^1.2.3"
  },
  "devDependencies": {
    "@alloc/fast-rimraf": "^1.0.8",
    "@alloc/prettier-config": "^1.0.0",
    "@types/node": "^14.14.37",
    "prettier": "^2.0.5",
    "strip-ansi": "^7.0.1",
    "tsup": "^5.11.9",
    "typescript": "^4.0.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./spin": {
      "types": "./dist/spin.d.ts",
      "node": "./spin.js",
      "default": "./stub.js"
    },
    "./task": {
      "types": "./dist/task.d.ts",
      "node": "./task.js",
      "default": "./stub.js"
    }
  }
}
