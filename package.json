{
  "name": "misty",
  "version": "1.5.0",
  "description": "Logging utils for command line",
  "author": "Alec Larson",
  "license": "MIT",
  "module": "index.mjs",
  "sideEffects": false,
  "repository": "alloc/misty",
  "prettier": "@alloc/prettier-config",
  "files": [
    "src",
    "*.js",
    "*.js.map",
    "*.mjs",
    "*.mjs.map",
    "*.d.ts",
    "!test.js"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "yarn build"
  },
  "dependencies": {
    "kleur": "^4.1.4",
    "shared-log": "^1.2.3"
  },
  "devDependencies": {
    "@alloc/prettier-config": "^1.0.0",
    "@types/node": "^14.14.37",
    "prettier": "^2.0.5",
    "strip-ansi": "^7.0.1",
    "tsup": "^5.11.9",
    "typescript": "^4.0.0"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./spin": {
      "require": "./spin.js",
      "import": "./spin.mjs",
      "types": "./spin.d.ts"
    },
    "./task": {
      "require": "./task.js",
      "import": "./task.mjs",
      "types": "./task.d.ts"
    }
  }
}
